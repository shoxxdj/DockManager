<% include static/head.ejs %>

<div class='row'>
	<div class='col-md-12'>
		<div class="panel panel-primary"><!-- Runnings Dockers -->
			<div class="panel-heading">	
				<h3 class="panel-title">Running Dockers</h3>
			</div>
			<div id="serverInfos" class="panel-body">

				<% var panel_colors=["default","default","default","default"]; %>
				<% var passages=0;
				   var server=1;
				for (var i=0;i<res.length;i++)
				{
					ip=res[i];
					i++;
					re=res[i];
				%>

					<%
						if(passages%3==0)
						{
				%>
				<div class="row">
				<%
						}	
				%>
					<div class="col-md-4" id='<%= server%>'> 
						<div class="panel panel-default">
							<div class="panel-heading">
								<div class="row">
								<div class="col-md-8">
									<b>	Server <%= server %> </b>
								</div>
								<div class="col-md-4">
								</div>
								</div>
							</div>
							<div class="panel-body">
								<table class="table table-striped">
									<tr>
										<td>Version</td>
										<td><%= re.Version %></td>
									</tr>
									<tr>
										<td>Os</td>
										<td><%= re.Os %></td>
									</tr>
									<tr>
										<td>Kernel</td>
										<td><%= re.KernelVersion %></td>
									</tr>
									<tr>
										<td>Go</td>
										<td><%= re.GoVersion %></td>
									</tr>
									<tr>
										<td>GitCommit</td>
										<td><%= re.GitCommit %></td>
									</tr>
									<tr>
										<td>Arch</td>
										<td><%= re.Arch %></td>
									</tr>
									<tr>
										<td>API</td>
										<td><%= re.ApiVersion %></td>
									</tr>


								</table>
							</div>
						</div>
					</div>
				<%
						if((passages+1)%3==0)
						{
				%>
				</div><!--end of created row -->
				<%
						}	
						passages++;
						server++;	
				}	
				if(passages%3==1)
					{
				%>
				</div> <!-- end of row ! -->
				<%
					}
				%>
				<% if(passages==0)
				{
				%>
				<center><h3><b>No active Servers !</b></h3><p>Edit conf.js AND RTFM !</p></center>
				<%
				}
				%>






			</div><!-- end of panel body -->
		</div><!--end of panel primary -->
	</div>
</div>

<% include static/foot.ejs %>